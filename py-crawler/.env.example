# Redis connection
# 支持两种格式: REDIS_ADDR (Python) 或 REDIS_REMOTE_ADDR (兼容 Go 爬虫)
REDIS_ADDR=localhost:6379
# REDIS_REMOTE_ADDR=100.99.127.100:6379
REDIS_PASSWORD=

# Rate limiting (与 Go Analyzer 保持一致)
APP_RATE_LIMIT=2
APP_RATE_BURST=5

# Crawler settings
CRAWLER_MAX_CONCURRENT_TASKS=3

# Server ports
HEALTH_PORT=8081
METRICS_PORT=2112

# Logging
LOG_LEVEL=info
